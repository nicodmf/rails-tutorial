<%= render 'shared/error_messages', :object => f.object %>
<% if inscription %>
  <%= f.label :username %><br />
  <%= f.text_field :username %>
<% else %>
	<div><%= :username%></div>
<% end%>
<div class="field">
  <%= f.label :nom %><br />
  <%= f.text_field :nom %>
</div>
<div class="field">
  <%= f.label :email %><br />
  <%= f.text_field :email %>
</div>
<div class="field">
  <%= f.label :password %><br />
  <%= f.password_field :password %>
</div>
<div class="field">
  <%= f.label :password_confirmation, "Confirmation" %><br />
  <%= f.password_field :password_confirmation %>
</div>